Flow org.gluu.agama.totp
     Basepath ""
// user authn
Log "@info user authn"
// Username password login
user | E = Trigger org.gluu.agama.pw 
Log "@info user details " user
userId = user.data.userId
Log "@info user id " userId
enrollData | E = Trigger org.gluu.agama.otp.enroll userId
Log "@info enroll data" enrollData